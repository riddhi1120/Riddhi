#include <iostream.h>
int main() {
    int num1, num2, num3;

    cout << "Enter three numbers:\n";
    cin >> num1 >> num2 >> num3;

    int max;

    if (num1 >= num2 && num1 >= num3)
        max = num1;
    else if (num2 >= num1 && num2 >= num3)
        max = num2;
    else
        max = num3;

    cout << "The maximum number is: " << max << endl;

    return 0;
}
